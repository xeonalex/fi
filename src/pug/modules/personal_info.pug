section(class="main-section")
	.main__container
		.back-button
			a(href="javascript:void(0)")
				<svg xmlns="http://www.w3.org/2000/svg" width="20" height="14">
					<path fill="#909090" fill-rule="evenodd" d="M6.525.2a.708.708 0 0 1 1.008.994L2.42 6.286h16.873a.7.7 0 0 1 .706.703.71.71 0 0 1-.706.713H2.42l5.113 5.082a.72.72 0 0 1 0 1.004.708.708 0 0 1-1.008 0L.2 7.491a.69.69 0 0 1 0-.994L6.525.2z"></path>
				</svg>
				<h3>Dashboard</h3>
		.my-account__nav
			.title-my-account.text-uppercase
				span My account
			ul.account-nav__list
				li.account-nav__item.active
					a(href="javascript:void(0)") Personal Info
				li.account-nav__item
					a(href="javascript:void(0)") Change password
				li.account-nav__item
					a(href="javascript:void(0)") Notification
		form(action="#" name="main-profile__form")
			.main-data__block
				.top-main__data
					.first-name__block
						.name_data_input
							p First Name
							span *
						.wrapper-input
							input(type="text" class="input_data input-first__name" placeholder="Marcus")
							<img src="static/img/content_profile/main-data__icon.png" alt="Img">
					.last-name__block
						.name_data_input
							p Last Name
							span *
						.wrapper-input
							input(type="text" class="input_data input-last__name" placeholder="Doe")
							img(src="static/img/content_profile/main-data__icon.png" alt="Img")
				.bottom-main__data
					.email-block
						.name_data_input
							p Email
							span *
						.wrapper-input
							input(type="text" class="input_data input-email" placeholder="example@examp.le")
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="10">
								<path fill="#969696" fill-rule="evenodd" d="M11.499 6.999a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"></path>
							</svg>
					.phone-block
						.phone-code__block
							.name_data_input
								p Phone code
								span *
							.wrapper-input
								input(type="text" class="input_data input-phone__code")
								<img src="static/img/content_profile/bottom-icon.png" alt="Img">
						.phone-number__block
							.name_data_input
								p Number
								span *
							.wrapper-input
								input(type="text" class="input_data input-phone__number" placeholder="4657253")
								<svg xmlns="http://www.w3.org/2000/svg" width="18" height="10">
									<path fill="#969696" fill-rule="evenodd" d="M11.499 6.999a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"></path>
								</svg>
			.adress-data__block
				.title__block.text-uppercase
					p Adress
				.top-adress__data
					.country-block
						.name_data_input
							p Country
							span *
						.wrapper-input
							input(type="text" class="input_data input-country" placeholder="United States")
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="10">
								<path fill="#969696" fill-rule="evenodd" d="M11.499 6.999a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"></path>
							</svg>
					.state-block
						.name_data_input
							p State
							span *
						.wrapper-input
							input(type="text" class="input_data input-state" placeholder="CA")
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="10">
								<path fill="#969696" fill-rule="evenodd" d="M11.499 6.999a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"></path>
							</svg>
					.town-block
						.name_data_input
							p Town
							span *
						.wrapper-input
							input(type="text" class="input_data input-town" placeholder="Los Angeles")
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="10">
								<path fill="#969696" fill-rule="evenodd" d="M11.499 6.999a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm-5 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z"></path>
							</svg>
				.middle-adress__data
					.street-block
						.name_data_input
							p Street
							span *
						.wrapper-input
							input(type="text" class="input_data input-street" placeholder="Tiana Turnpike")
